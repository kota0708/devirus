{
	"env": {
		"es6": true,
		"browser": true,
		"jquery": true
	},
	"extends": [
		"eslint:recommended"
	],
  "plugins": [
    "@typescript-eslint",
    "typescript"
  ],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module",
    "allowImportExportEverywhere": true,
    "project": "./tsconfig.json"
  },
  "rules": {
		"no-console": "off",
		"no-undef": "off"
  }
}